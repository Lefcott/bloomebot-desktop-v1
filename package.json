  "main": "public/electron.js",
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "debug": "^4.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "iohook": "^0.6.6",
    "iohook-prebuild-test": "^0.1.16",
    "node-ipc": "^9.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "remote": "^0.2.6",
    "robotjs": "^0.6.0"
  },
  "iohook": {
    "targets": [
      "node-79",
      "electron-76"
    ],
    "platforms": [
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron -r @babel/register .",
    "electron-rebuild": "$(npm bin)/electron-rebuild",
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron -r @babel/register .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "9.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "concurrently": "^5.3.0",
    "electron": "^9.3.1",
    "electron-builder": "^22.8.0",
    "electron-rebuild": "^2.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-pug": "^1.2.2",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.2.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "prettier-eslint": "^8.2.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^9.0.2",
    "wait-on": "^5.2.0"
  }
}
